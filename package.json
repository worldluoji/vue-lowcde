{
  "name": "vue-lowcde",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "scripts": {
    "dev": "pnpm -r --filter=./packages/platform dev",
    "build": "pnpm -r --filter=./packages/platform build",
    "preview": "pnpm -r --filter=./packages/platform preview",
    "lint": "eslint --ext .js,.jsx,.vue ./",
    "fix": "eslint --fix --ext .js,.jsx,.vue ./",
    "build:comp": "pnpm -r --filter=./packages/customer build",
    "build:engine": "pnpm -r --filter=./packages/runtime build",
    "preview:engine": "pnpm -r --filter=./packages/runtime preview",
    "dev:engine": "pnpm -r --filter=./packages/runtime dev",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/worldluoji/vue-lowcde.git"
  },
  "workspaces": [
    "packages/platform",
    "packages/runtime",
    "packages/elements",
    "packages/helper",
    "packages/customer",
    "packages/viteplugins",
    "packages/request"
  ],
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/worldluoji/vue-lowcde/issues"
  },
  "homepage": "https://github.com/worldluoji/vue-lowcde#readme",
  "dependencies": {
    "install": "^0.13.0",
    "throttle-debounce": "^5.0.0",
    "uuid": "^9.0.0",
    "vue": "^3.2.45"
  },
  "devDependencies": {
    "@lowcode/elements": "workspace:*",
    "@lowcode/helper": "workspace:*",
    "@vitejs/plugin-legacy": "^3.0.1",
    "@vitejs/plugin-vue": "^4.0.0",
    "autoprefixer": "^10.4.13",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-vue": "^9.8.0",
    "husky": "^8.0.0",
    "prettier": "^2.8.2",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-external-globals": "^0.7.1",
    "sass": "^1.57.1",
    "terser": "^5.16.1",
    "unplugin-auto-import": "^0.12.0",
    "unplugin-vue-components": "^0.22.11",
    "vite": "^4.0.0",
    "vite-plugin-chunk-split": "^0.4.7"
  }
}
